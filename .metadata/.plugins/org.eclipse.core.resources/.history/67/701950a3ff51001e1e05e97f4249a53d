	 <!-- Constructor injection -->
	 
	 <bean class="practice.set.Trainer" id="trainer">
	 	<constructor-arg name="id" value="1"></constructor-arg>
	 	<constructor-arg name="name" value="Rohan"></constructor-arg>
	 	<constructor-arg name="subjectsTaught">
			<list>
				<value>Math</value>
				<value>Science</value>
			</list>
		</constructor-arg> 
	 </bean>
	
	<bean class="practice.set.Learner" id="learner">
		<constructor-arg value="1" type="int" name="id"></constructor-arg>  
		<constructor-arg value="Rohan" name="name"></constructor-arg>  
		<constructor-arg name="phoneNumbers">
			<list>
				<value>9876543210</value>
			</list>
		</constructor-arg>
		<constructor-arg name="batchCode" value="B123"></constructor-arg>  
		<constructor-arg >
			<ref bean="trainer"></ref >
		</constructor-arg>
		
	</bean>


